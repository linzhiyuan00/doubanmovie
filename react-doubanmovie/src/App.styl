.App
  width 100%
  height 100%
  color #000000
  overflow hidden
  .header
    color: #ffd700
    height 50px
    line-height: 50px
    text-align: center
    font-size 20px
    background-color #c62f2f
  .tabbar
    position: fixed
    bottom 0
    display: flex
    width 100%
    height 48px
    border-top: 1px solid #e8e8e8
    background-color #f8f8f8
    color: #a0a0a0
    .tabbar-item
      flex: 1
      height 100%
      padding-top: 5px
      line-height: 12px
      font-size 10px

.nav-link, .link
  display: block
  color:#000
  text-decoration: none
.active
  color: #FFD700
  transform: scale(1.3)

    