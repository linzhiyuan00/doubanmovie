.music-header
  position: fixed
  width: 100%
  height: 50px
  color: #ffffff
  text-align: center
  font-size: 18px
  background-color #c62f2f
  .header-back
    position: absolute
    top: 14px
    left: 10px
    font-size: 22px
    color: #ffffff
    z-index: 999
  .header-title
    margin: 0 40px
    line-height: 50px
    overflow: hidden
    text-overflow: ellipsis
    white-space: nowrap
